<body>
    <h1>List Page</h1>
    <hr />
    <a href="/add">데이터추가</a>
    <table width="100%" border="1">
        <tr>
            <th>번호</th>
            <th>제목</th>
            <th>출판사</th>
            <th>가격</th>
            <th>수정/삭제</th>
        </tr>
        <% data.forEach(function(item, index) { %>
        <tr>
            <td><%= item.bookid %></td>
            <td><%= item.bookname %></td>
            <td><%= item.publisher %></td>
            <td><%= item.price %></td>
            <td><a href="/edit">수정</a> <a href="/del/<%= item.bookid %>">삭제</a></td>
        </tr>
        <% }); %>
    </table>
</body>